= content_for :title, @post.title
= content_for :meta do 
  = render partial: 'posts/opengraph', locals: { post: @post }
= content_for :nav do
  = render partial: 'public/nav', locals: { user: @user }
= content_for :launchpad do
  - if user_signed_in?
    = render partial: 'posts/launchpad', locals: { user: current_user, post: @post } 
  
= render partial: 'posts/post', locals: {post: @post, preview: false}
= render partial: 'shared/google_analytics' if Rails.env.production?